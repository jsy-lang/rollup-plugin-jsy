{
  "name": "rollup-plugin-jsy",
  "version": "1.8.0",
  "description": "Rollup JSY syntax transpiler to standard JavaScript",
  "license": "MIT",
  "author": "Shane Holloway",
  "homepage": "https://github.com/jsy-lang/rollup-plugin-jsy#readme",
  "keywords": [
    "rollup",
